CREATE DATABASE carsubdata;

USE carsubdata;

CREATE TABLE car(
    carId int AUTO_INCREMENT,
    carName varchar(1000),
    PRIMARY KEY (carId)
);

CREATE TABLE carsubscription(
    name varchar(50),
    lastName varchar(50),
    address varchar(100),
    email varchar(100),
    phoneNumber int,
    id int AUTO_INCREMENT,
    carId int,
    PRIMARY KEY (id),
    FOREIGN KEY (carId) REFERENCES car (carId)
);

